{
  "name": "marketplace-app-for-good",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm supabase:start && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier:check": "prettier --check \"./**/*.{js,ts,jsx,tsx,json}\"",
    "prettier:write": "prettier --write \"./**/*.{js,ts,jsx,tsx,json}\"",
    "test": "vitest",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:db-push": "npx supabase db push",
    "supabase:generate-types": "npx supabase gen types typescript --project-id $PROJECT_REF --schema public > src/types/supabase.ts",
    "supabase:reset": "npx supabase db reset --local",
    "supabase:reset:debug": "npx supabase db reset --local --debug",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.556.0",
    "@aws-sdk/lib-storage": "^3.556.0",
    "@changesets/cli": "^2.27.1",
    "@hookform/resolvers": "^3.4.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/themes": "^3.0.2",
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.39.7",
    "await-to-js": "^3.0.0",
    "geist": "^1.2.2",
    "next": "14.1.2",
    "pino": "^8.19.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.5",
    "sonner": "^1.4.41",
    "stripe": "^14.19.0",
    "zod": "^3.23.8",
    "zod-form-data": "^2.0.2"
  },
  "devDependencies": {
    "@testing-library/react": "^14.2.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.0.1",
    "encoding": "^0.1.13",
    "eslint": "^8",
    "eslint-config-next": "14.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "husky": "^8.0.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "vitest": "^1.3.1"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
